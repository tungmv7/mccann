<wpml-config>
    <custom-types>
        <custom-type translate="1">portfolio</custom-type>
        <custom-type translate="1">testimonials</custom-type>
    </custom-types>    
    <taxonomies>
        <taxonomy translate="1">project-type</taxonomy>
    </taxonomies>
  <custom-fields>
    <custom-field action="copy">_cmb_theme_style</custom-field>
    <custom-field action="copy">_cmb_post_sidebar_layout</custom-field>
    <custom-field action="translate">name</custom-field>
    <custom-field action="translate">url</custom-field>
    <custom-field action="copy">portf_thumbnail</custom-field>
    <custom-field action="copy">link_type</custom-field>
    <custom-field action="translate">project_desc</custom-field>
    <custom-field action="translate">_cmb_projects-custom-fields</custom-field>
    <custom-field action="ignore">project_client</custom-field>
    <custom-field action="ignore">project_website</custom-field>
    <custom-field action="copy">_cmb_project_gallery_images</custom-field>
    <custom-field action="copy">_cmb_project_is_thumb_include</custom-field>
    <custom-field action="copy">_cmb_project_is_slider</custom-field>
    <custom-field action="translate">testimonial_content</custom-field>
    <custom-field action="translate">_cmb_alternate_title</custom-field>
    <custom-field action="copy">_cmb_menu_style</custom-field>
  </custom-fields>   
  <admin-texts>
    <key name="theme_data">
      <key name="logo">
        <key name="url"/>
      </key>
      <key name="retina_logo">
        <key name="url"/>
      </key>
      <key name="favicon">
        <key name="url"/>
      </key>
      <key name="footer_text"/>
    </key>
  </admin-texts>  

    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#f4f4f4</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#ffffff</key>
            <key name="border">transparent</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>    
</wpml-config>